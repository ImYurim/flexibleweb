   <link rel="stylesheet" href="/stylesheets/bootstrap.css">
   <link rel="stylesheet" href="/fonts/NotoSansCJKkr-Medium.otf">
   <style>
          .navbar {
      padding-right: 24px;
      padding-left: 5px;
    }


      .top-button {
         display: inline-flex;
         justify-content: center;
         align-items: center;
         background: transparent;
         margin: 0;
         outline: none;
         border: none;
         color: #242e64;
         font-size: 16px;
         font-weight: 400;
         line-height: 1.5;
         cursor: pointer;
         opacity: 1;
         margin-right: 13px;
      }

      .logo {
         margin-bottom: 14px;
         opacity: 1;
      }

      @media screen and (min-width:1200px){
      .select-wrap {
         display: flex;
         align-items: center;
         border-left: 1px solid #dcdfee;
         padding: 0 0 0 12px;
         height: 32px;
         margin-left: 24px;
      }
      }


      @media screen and (max-width:1199px){
      .select-wrap {
         display: none;
      }
      }


      .select-wrap>a {
         margin-right: 14px;
         color: #7B81A2;
         margin-left: 24px;
         opacity: 1;
         letter-spacing: 0px;
         font-family: 'Noto Sans CJK KR Regular', sans-serif;
         font-size: 18px;
         margin-top: 2px;
      }
      .float-right {
            display: flex;
            margin-left: auto;
            padding: 0 0 0 24px;

        }
        .float-right>a {
            color: #7B81A2;
            margin-left: 24px;
            opacity: 1;
            letter-spacing: 0px;
            font-family: 'Noto Sans CJK KR Regular', sans-serif;
            font-size: 18px;
            margin-top: 7px;
        }
        .float-right > img {
            margin-left: 24px;
        }





      .search-box>button {
         margin: 0 4px;
         padding: 4px;
         width: 32px;
         height: 32px;
      }

      @media screen and (max-width:820px){
            .search-box{
                display: none;

            }
            .float-right > a {
                display: none;
            }
            .logo{
                display:none;
            }
 
        }

        @media screen and (min-width:821px){
    .search-box {
      width: 280px;
      height: 40px;
      display: flex;
      align-items: center;

    }
    }
        
        .form-control-nav {
            width: 100%;
            display: flex;
            height: calc(1.5em + 0.75rem + 2px);
            color: #394273;
            padding: 0 40px 0 12px;
            font-size: 1rem;
            font-weight: 400;
            line-height: 1.5;
            background: #f5f6ff 0% 0% no-repeat padding-box;
            background-clip: padding-box;
            border: 1px solid #ced4da;
            border-radius: 0.25rem;
            transition: border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out;
        }

      .footer.dark {
         background: #131830;
         font-family: 'Noto Sans CJK KR Regular', sans-serif;
         border-top: 1px solid #1b2246;
         color: hsla(0, 0%, 100%, .6);
      }

      .column {
         display: flex;
         flex-direction: column;
         margin: 0 24px;
         position: relative;
         width: 100%;
         max-width: 1200px;
      }

      .linkBox {
         margin-top: 50px;
         margin-bottom: 20px;
      }

      .addressbox>p {
         font-family: 'Noto Sans CJK KR Regular', sans-serif;
      }

      .column>a {
         font-family: 'Noto Sans CJK KR Regular', sans-serif;
      }

      .searchButton {
            position: absolute;
            top: 56%;
            right: 256px;
            transform: translateY(-50%);
            background: none;
            border: none;
            border-radius: 4px;
            margin: 0 4px;
            padding: 4px;
            width: 30px;
            height: 30px;
            background-image: url(/images/searchglass.svg);
            background-repeat: no-repeat;
            }


      .linkBox>a {
         color: hsla(0, 0%, 100%, .6);
         font-family: 'Noto Sans CJK KR Regular', sans-serif;
         text-decoration: none;
         background-color: transparent;
      }

      .components>li>a {
         color: #394273;
         text-decoration: none;
         background-color: transparent;
      }

      .side-item>.list>.column>.link:before {
         position: absolute;
         left: 24px;
         content: "";
         background: #dcdfee;
         width: 1px;
         height: 100%;
      }

      .components>li {
         margin-left: 12px;
      }

      .side-a {
         color: gray;
         border-left: 1px solid #dcdfee;
         padding-left: 14px;
         height: 54px;
      }

      .main-window {
         /* position: relative; */
         background: linear-gradient(150deg, #242e64, #131830);
         width: 100%;
         overflow-x: hidden;
      }

      .bg-wrapper {
         z-index: 0;
         position: absolute;
         width: 100%;
         height: auto;
      }

      .bg-wrapper>img {
         height: 100%;
         width: 100%;
      }

      .main-title {
         font-size: 36px;
         line-height: 48px;
         margin: 80px 0 0;
         color: #fff;
         font-weight: 700;
         text-align: center;
      }

      .main-title>.accent {
         font-size: 48px;
         font-weight: 900;
      }

      .main-window>.main-icon {
         margin: 0 auto;
         display: block;
      }

      .sidebar-header>img {
         margin-top: 22px;
         padding-left: 24px;
      }

      .modal-login-header {
         justify-content: space-between;
         padding: 1rem 1rem;
         border-top-left-radius: calc(0.3rem - 1px);
         border-top-right-radius: calc(0.3rem - 1px);
      }



      .loginImage>img {
         margin: 10px 0 12px;
         width: 42px;
         height: 28px;
      }

      h5 {
         font-family: 'Noto Sans CJK KR Regular', sans-serif;
         margin: 0 0 24px;
         color: #242e64;
         font-size: 20px;
         font-weight: 700;
      }

      .button-primary {
         background: #394dd1;
         border-radius: 4px;
         color: #fff;
         margin: 24px 0 0;
         width: 100%;
         height: 48px;
      }

      .Joinheader {
         margin-top: 30px;
      }
   </style>
