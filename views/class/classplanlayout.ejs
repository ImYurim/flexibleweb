<script>
  $(document).ready(function () {
    var slider = document.getElementById("recommendslider");
    var basic = document.getElementById("recommendbasic");

    if ($(this).width() < 600) {
      slider.style.display = 'block';
      basic.style.display = 'none';
    }
    else {
      slider.style.display = 'none';
      basic.style.display = 'block';
    }

    $(window).resize(function () {
      var slider = document.getElementById("recommendslider");
      var basic = document.getElementById("recommendbasic");

      if ($(this).width() < 600) {
        slider.style.display = 'block';
        basic.style.display = 'none';
      }
      else {
        slider.style.display = 'none';
        basic.style.display = 'block';
      }
    })

    $('.scrabicon').click(function () {
      var scrabicon = $(this).data('value');
      if (scrabicon == "checked") {
        $(this).html('<img class="scrabicon" src="/images/class/bookmark.png" />');
        $(this).data('value', "notchecked");
      }
      else {
        $(this).html('<img class="scrabicon" src="/images/class/bookmarkchecked.png" />');
        $(this).data('value', "checked");

      }
    })

    $(".searchkeyword").click(function () {
         var langbtn = $(this).val();
         document.getElementById("keywordinput").value = langbtn;
      });

    




  })

</script>


