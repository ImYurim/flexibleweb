            $('#sidebar').removeClass('active');
        });

        $('#sidebarCollapse').on('click', function () {
            $('#sidebar').addClass('active');
            $('.collapse.in').toggleClass('in');
            $('a[aria-expanded=true]').attr('aria-expanded', 'false');
        });

        $("#joinform").click(function () {
            location.replace('/joinform');
        });



        $(".title").click(function () {
         var langbtn = $(this).val();
         document.getElementById("keywordinput").value = langbtn;
      });



      $(".maintitle").click(function () {
         var title = $(this).html();
         if(title == "프로필정보"){
             document.getElementById("form1").style.display='block';
             document.getElementById("form2").style.display='none';
             document.getElementById("form3").style.display='none';
             document.getElementById("jointitle1").style.fontWeight='bold';
             document.getElementById("jointitle2").style.fontWeight='normal';
             document.getElementById("jointitle3").style.fontWeight='normal';
             document.getElementById("jointitle1").style.borderBottom='1px solid lightgray';
             document.getElementById("jointitle2").style.borderBottom='0px';
             document.getElementById("jointitle3").style.borderBottom='0px';

         }
         else if (title == "스웨팅 정보"){
            document.getElementById("form1").style.display='none';
            document.getElementById("form2").style.display='block';
            document.getElementById("form3").style.display='none';
            document.getElementById("jointitle1").style.fontWeight='normal';
             document.getElementById("jointitle2").style.fontWeight='bold';
             document.getElementById("jointitle3").style.fontWeight='normal';
             document.getElementById("jointitle1").style.borderBottom='0px';
             document.getElementById("jointitle2").style.borderBottom='1px solid lightgray';
             document.getElementById("jointitle3").style.borderBottom='0px';

         }
         else{
            document.getElementById("form1").style.display='none';
             document.getElementById("form2").style.display='none';
             document.getElementById("form3").style.display='block';
             document.getElementById("jointitle1").style.fontWeight='normal';
             document.getElementById("jointitle2").style.fontWeight='normal';
             document.getElementById("jointitle3").style.fontWeight='bold';
             document.getElementById("jointitle1").style.borderBottom='0px';
             document.getElementById("jointitle2").style.borderBottom='0px';
             document.getElementById("jointitle3").style.borderBottom='1px solid lightgray';

         }
      });

    

    
    });



    
</script>

